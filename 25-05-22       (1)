#include<stdio.h>
void main()
{
    char str[50];
    int j=0,k=0,l=0;
    gets(str);
    int n=strlen(str);
    for(int i=0;i<n;i++)
    {
        if(isupper(str[i])) j++;
        else if(islower(str[i])) k++;
        else if(isdigit(str[i])) l++;
        else continue;
    }
    printf("Count of Uppercase characters: %d\n",j);
    printf("Count of lowercase characters: %d\n",k);
    printf("Count of numbers: %d\n",l);
