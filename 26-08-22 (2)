#include<stdio.h>
#include<string.h>
void main()
{
char a[20];
int i,j,size,count=0,p;
clrscr();
printf("enter the value\n");
gets(a);
size=strlen(a);
p=size-1;
for(i=0;i<p/2;i++)
{
for(j=p-i;j>0;j--)
if(a[i]==a[j]){
count++;
break;}
}
if(count==(size-1)/2)
printf("PALINDROME");
else
printf("NOT A PALINDROME");
}
